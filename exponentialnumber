import java.util.Scanner;

public class exponentialnumber {
    public static void main(String[]args){

        int a,b;

        Scanner sc = new Scanner(System.in);

        System.out.print("Tabandaki sayiyi giriniz: ");
        a=sc.nextInt();
        System.out.print("Us konumundaki sayiyi giriniz: ");
        b=sc.nextInt();
        if(b>1) {
            for (int i = 1; i < b; i++) {
                a *= a;
                System.out.print(a);
            }
        }else if(b==1){
            System.out.print(a);
        }else if(b==0){
            System.out.print("1");
        }else{
            System.out.print("Bu program negatif usleri hesaplayamaz");
        }
    }
}
