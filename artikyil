import java.util.Scanner;

public class artikyil {
    public static void main(String[]args){

        Scanner input= new Scanner(System.in);

        int yil;

        System.out.println("Artik yil oldugunu kontrol etmek icin yil giriniz: ");
        yil = input.nextInt();

        if(yil%4==0){
            if(yil%100==0){
                if(yil%400==0){
                    System.out.println("Girdiginiz tarih bir artik yildir.");
                }else{
                    System.out.println("Girdiginiz tarih bir artik yil degildir");
                }

                }else if(yil%100!=0){
                System.out.println("Girdiginiz tarih bir artik yildir.");
            }
        } else{
            System.out.println("Girdiginiz tarih bir artik yil degildir");
        }
    }
}
