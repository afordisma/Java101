import java.util.Scanner;

public class poweroffourandfive {
    public static void main(String... arg){

        Scanner in = new Scanner(System.in);

        int a,b;

        System.out.print("Maksimum sayiyi girin: ");
        a=in.nextInt();

        for(int i = 1 ; i<=a ; i *=4){

            b=(5*i)/4;

            System.out.println("4.kuvveti: "+i);
            System.out.println("5.kuvveti: "+b);

        }

    }
}
